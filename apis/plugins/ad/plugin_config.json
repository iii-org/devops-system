{
  "name": "ad",
  "default_disabled": true,
  "is_pipeline": false,
  "keys": [
    {
      "key": "host",
      "type": "string",
      "store": "secret_system"
    },
    {
      "key": "ou",
      "type": "string",
      "store": "secret_system"
    },
    {
      "key": "domain",
      "type": "string",
      "store": "secret_system"
    },
    {
      "key": "account",
      "type": "string",
      "store": "secret_system"
    },
    {
      "key": "password",
      "type": "password",
      "store": "secret_system"
    },
    {
      "key": "default_password",
      "type": "string",
      "store": "secret_system"
    },
    {
      "key": "connect_timeout",
      "type": "string",
      "store": "secret_system"
    },
    {
      "key": "default_role_id",
      "type": "select",
      "options": [
        {
          "value": 1,
          "name": "Engineer"
        },
        {
          "value": 3,
          "name": "Project Manager"
        },
        {
          "value": 5,
          "label": "Administrator"
        },
        {
          "value": 7,
          "name": "QA"
        }
      ],
      "store": "secret_system",
      "placeholder": "Please Select default role ID"
    },
    {
      "key": "ssl",
      "type": "select",
      "options": [
        {
          "value": true,
          "name": "True"
        },
        {
          "value": false,
          "name": "False"
        }
      ],
      "store": "secret_system"
    },
    {
      "key": "ssl_validate",
      "type": "select",
      "options": [
        {
          "value": "NONE",
          "name": "Certificate Not Validated"
        },
        {
          "value": "OPTIONAL",
          "name": "Certificate not required, but validated if provided"
        },
        {
          "value": "REQUIRED",
          "name": "Certificate Required and Validated"
        }
      ],
      "store": "secret_system"
    },
    {
      "key": "ca_certs_data",
      "type": "password",
      "store": "secret_system"
    }
  ]
}