{
  "name": "ad",
  "default_disabled": true,
  "keys": [
    {
      "key": "host",
      "type": "string",
      "store": "secret_system"
    },
    {
      "key": "ou",
      "type": "string",
      "store": "secret_system"
    },
    {
      "key": "domain",
      "type": "string",
      "store": "secret_system"
    },
    {
      "key": "account",
      "type": "string",
      "store": "secret_system"
    },
    {
      "key": "password",
      "type": "string",
      "store": "secret_system"
    },
    {
      "key": "default_password",
      "type": "string",
      "store": "secret_system"
    },
    {
      "key": "connect_timeout",
      "type": "string",
      "store": "secret_system"
    },
    {
      "key": "default_role_id",
      "type": "select",
      "options": [
        {
          "id": 1,
          "name": "Engineer"
        },
        {
          "id": 3,
          "name": "Project Manager"
        },
        {
          "id": 5,
          "label": "Administrator"
        },
        {
          "value": 7,
          "name": "QA"
        }
      ],
      "store": "secret_system"
    },
    {
      "key": "ssl",
      "type": "select",
      "options": [
        {
          "value": true,
          "name": "True"
        },
        {
          "value": false,
          "name": "False"
        }
      ],
      "store": "secret_system"
    },
    {
      "key": "ssl_validate",
      "type": "select",
      "options": [
        {
          "value": "CERT_NONE",
          "name": "certificates are ignored"
        },
        {
          "value": "CERT_OPTIONAL",
          "name": "certificate not required, but validated if provided"
        },
        {
          "value": "CERT_REQUIRED",
          "name": "required and validated"
        }
      ],
      "store": "secret_system"
    },
    {
      "key": "ca_certs_data",
      "type": "string",
      "store": "secret_system"
    }
  ]
}